<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lgf.mapper.UsersMapper" >
    <insert id="insertUser" parameterType="com.lgf.pojo.Users">
        insert into users(name,age) values(#{username},#{userage})
    </insert>
    <select id="selectUserAll" resultType="com.lgf.pojo.Users">
        select * from users
    </select>
</mapper>